<%@ page language="java" contentType="text/html; charset=utf-8" %>

<div class="login-container animated fadeInDown">
        <div class="loginbox bg-white">
            <div class="loginbox-title">Allets Data Dash Board Login</div>
            <!--id form-->
            <div class="loginbox-textbox">
                <input type="text" id="name" class="form-control" placeholder="ID" />
            </div>
            <!--/id form-->
            <!--password form-->
            <div class="loginbox-textbox">
                <input type="password" id="password" class="form-control" placeholder="Password" />
            </div>
            <!--/password form-->
            <!--login-->
            <div class="loginbox-submit">
                <input type="button" class="btn btn-primary btn-block" value="Login">
            </div>
            <!--/login-->
        </div>
    </div>

    <!--Basic Scripts-->
    <script src="/js/jquery.min.js"></script>
    <script src="/js/data/jquery.md5.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="/js/jquery.cookie.min.js"></script>

    <!--Beyond Scripts-->
    <script src="/js/beyond.js"></script>
<!--i18n-->
<script src="/js/jquery.i18n.properties.js" charset=“UTF-8”></script>
<script src="/js/data/i18n.js"></script>

